    <declare-styleable name="IndexFastScrollRecyclerView">
        <attr name="setIndexTextSize" format="integer" />
        <attr name="setIndexbarWidth" format="float" />
        <attr name="setIndexbarMargin" format="float" />
        <attr name="setPreviewPadding" format="integer" />
        <attr name="setPreviewTextSize" format="integer" />
        <attr name="setPreviewColor" format="color|string" />
        <attr name="setPreviewTextColor" format="color|string" />
        <attr name="setPreviewTransparentValue" format="float" />
        <attr name="setIndexBarCornerRadius" format="integer" />
        <attr name="setIndexBarTransparentValue" format="float" />
        <attr name="setIndexBarShown" format="boolean" />
        <attr name="setIndexBarColor" format="color|string" />
        <attr name="setIndexBarTextColor" format="color|string" />
        <attr name="setIndexBarHighlightTextColor" format="color|string" />
        <attr name="setIndexBarStrokeWidth" format="integer" />
        <attr name="setIndexBarStrokeColor" format="color|string" />
    </declare-styleable>
    <declare-styleable name="ItemSettingView">
        <attr name="checkable" format="boolean"/>
        <attr name="title" format="string"/>
        <attr name="subtitle" format="string"/>
        <attr name="drawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FloatSeekBar">
        <attr name="floatMax" format="float" />
        <attr name="floatMin" format="float" />
    </declare-styleable>

    <declare-styleable name="SquareImageView">
        <attr name="mode" format="integer">
            <enum name="square_by_width" value="0" />
            <enum name="square_by_height" value="1" />
            <enum name="square_by_xy" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RatioFrameLayout">
        <attr name="ratioBy" format="integer">
            <enum name="auto" value="0" />
            <enum name="width" value="1" />
            <enum name="height" value="2" />
        </attr>
        <attr name="ratio" format="float" />
    </declare-styleable>
    <declare-styleable name="MenuItem">
        <attr name="menuIcon" format="integer" />
        <attr name="menuName" format="string" />
        <attr name="showCheckbox" format="boolean" />
    </declare-styleable>
    <declare-styleable name="EmptyAdView">
        <attr name="emptyText" format="string|reference" />
        <attr name="emptyTextColor" format="color" />
        <attr name="hideEmptyText" format="boolean" />
    </declare-styleable>

    <declare-styleable name="cs_CircularSeekBar">
        <attr name="cs_circle_style">
            <enum name="butt" value="0" />
            <enum name="round" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="cs_progress" format="integer"/>
        <attr name="cs_max" format="integer"/>
        <attr name="cs_negative_enabled" format="boolean"/>
        <attr name="cs_move_outside_circle" format="boolean"/>
        <attr name="cs_maintain_equal_circle" format="boolean"/>
        <attr name="cs_use_custom_radii" format="boolean"/>
        <attr name="cs_lock_enabled" format="boolean"/>
        <attr name="cs_circle_x_radius" format="dimension" />
        <attr name="cs_circle_y_radius" format="dimension" />
        <attr name="cs_circle_stroke_width" format="dimension" />
        <attr name="cs_disable_pointer" format="boolean"/>
        <attr name="cs_pointer_stroke_width" format="dimension" />
        <attr name="cs_pointer_halo_width" format="dimension" />
        <attr name="cs_pointer_halo_border_width" format="dimension"/>
        <attr name="cs_circle_fill" format="color"/>
        <attr name="cs_circle_color" format="color"/>
        <attr name="cs_circle_progress_color" format="color"/>
        <attr name="cs_pointer_color" format="color"/>
        <attr name="cs_pointer_halo_color" format="color"/>
        <attr name="cs_pointer_halo_color_ontouch" format="color"/>
        <attr name="cs_pointer_alpha_ontouch" format="integer"/>
        <attr name="cs_pointer_angle" format="float"/>
        <attr name="cs_start_angle" format="float"/>
        <attr name="cs_end_angle" format="float"/>
        <attr name="cs_disable_progress_glow" format="boolean"/>
        <attr name="cs_hide_progress_when_empty" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ItemSongWidget">
        <attr name="item_song_widget_position" format="string"/>
        <attr name="item_song_widget_song_name" format="string"/>
        <attr name="item_song_widget_artist_name" format="string"/>
    </declare-styleable>
